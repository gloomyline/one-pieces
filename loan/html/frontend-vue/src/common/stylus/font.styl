@import "fontVariables"

@font-face
  font-family 'wk-loans-app-icons'
  src url(icomoon-font-path + '/wk-loans-app-icons.eot?xt03zh')
  src url(icomoon-font-path + '/wk-loans-app-icons.eot?xt03zh#iefix') format('embedded-opentype'), url(icomoon-font-path + '/wk-loans-app-icons.ttf?xt03zh') format('truetype'), url(icomoon-font-path + '/wk-loans-app-icons.woff?xt03zh') format('woff'), url(icomoon-font-path + '/wk-loans-app-icons.svg?xt03zh#wk-loans-app-icons') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'wk-loans-app-icons' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

