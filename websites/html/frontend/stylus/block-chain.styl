.clearfix:after,
.clearfix:before
    content: ''
    display: table
.clearfix
    clear: both

.definition
    min-width: 1286px
    text-align: center
    font-family: '黑体'
    .pro-title
        font-size: 22px
        font-weight: bold
        color: #194d9e
    .tendency
        padding-top: 122px
        .desc
            width: 1006px
            margin: 0 auto
            padding-top: 35px
            color: #333333
            font-size: 16px
