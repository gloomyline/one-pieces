@import "fontVariables"

@font-face
  font-family 'wk-times-icon'
  src url(icomoon-font-path + '/wk-times-icon.eot?jc0m7k')
  src url(icomoon-font-path + '/wk-times-icon.eot?jc0m7k#iefix') format('embedded-opentype'), url(icomoon-font-path + '/wk-times-icon.ttf?jc0m7k') format('truetype'), url(icomoon-font-path + '/wk-times-icon.woff?jc0m7k') format('woff'), url(icomoon-font-path + '/wk-times-icon.svg?jc0m7k#wk-times-icon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'wk-times-icon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

